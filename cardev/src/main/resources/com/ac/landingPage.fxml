<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ac.menus.mainMenu">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="720.0" fitWidth="1280.0">
               <image>
                  <Image url="@images/Main1.png" />
               </image>
            </ImageView>
            <ListView layoutX="515.0" layoutY="260.0" opacity="0.47" prefHeight="200.0" prefWidth="292.0" />
            <Button fx:id="btnNewSeason" layoutX="621.0" layoutY="291.0" mnemonicParsing="false" onAction="#startNewSeason" text="New Season" />
            <Button fx:id="btnLoadSeason" layoutX="699.0" layoutY="348.0" mnemonicParsing="false" onAction="#btnLoadSeasonClick" text="Load Season" />
            <ComboBox fx:id="cmbSelectSeason" layoutX="541.0" layoutY="348.0" onAction="#cmbSelectSeasonFunc" prefWidth="150.0" promptText="Select Season" />
            <Button fx:id="btnQuit" layoutX="642.0" layoutY="401.0" mnemonicParsing="false" onAction="#btnCloseWindow" text="Quit" />
         </children>
    </AnchorPane>
  </children>
</VBox>
