<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="250.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ac.newVersion">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="131.0" layoutY="36.0" text="New version available!">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Button fx:id="btnVersion" layoutX="81.0" layoutY="199.0" mnemonicParsing="false" onAction="#btnVersionClick" text="Get new version" />
            <Button fx:id="btnContinue" layoutX="481.0" layoutY="199.0" mnemonicParsing="false" onAction="#btnContinueClick" text="Continue" />
            <Label contentDisplay="CENTER" layoutX="123.0" layoutY="108.0" text="A new version of Assetto Corsa: Formula Championship has been released!" />
            <Label contentDisplay="CENTER" layoutX="147.0" layoutY="137.0" text="Click &quot;Get new version&quot; to go to get the latest version on Patreon." />
         </children>
    </AnchorPane>
  </children>
</VBox>
